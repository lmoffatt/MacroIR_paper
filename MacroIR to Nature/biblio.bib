@article{08f5885602ed975eaa5c89aff4d60ad3007f6d29,
	title = {Ion Channel Modeling},
	year = {2005},
	url = {https://www.semanticscholar.org/paper/08f5885602ed975eaa5c89aff4d60ad3007f6d29},
	abstract = {Electrical activity in the nervous system is regulated by the passage of minute electrical currents due to charged ions passing though ion channels in cell membranes. Studying them increases our understanding of normal physiology and the effect of drugs and toxins. 
	
	
	
	Stochastic modeling of ion channel mechanisms is needed to infer the nature of the mechanism from observation of ionic currents, and to estimate the parameters involved. The models are Markov or semi-Markov processes where, typically, states are aggregated into groups with the same conductance level because we can see what level it is at, not what state it is in. 
	
	
	
	Many distributions can be obtained, such as open times or shut times, and the likelihood can be calculated for a record of sojourns at the various levels that are first extracted from observational noise. Hidden Markov methods have also been applied. 
	
	
	Keywords: 
	
	ion channel; 
	aggregated Markov process; 
	semi-Markov process; 
	hidden Markov methods; 
	time interval omission; 
	biophysics; 
	nervous system},
	author = {A. Hawkes},
	doi = {10.1002/9781118445112.STAT05070},
}

