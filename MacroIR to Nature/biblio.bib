@article{225cc2c0c325a8b4a890081e4e59ec08f98b9900,
	title = {Statistical Inference for a Two‐State Markov Model of a Single Ion Channel, Incorporating Time Interval Omission},
	year = {1995},
	url = {https://www.semanticscholar.org/paper/225cc2c0c325a8b4a890081e4e59ec08f98b9900},
	abstract = {A two-state Markov model of a single ion channel, in which successive open and closed sojourns follow negative exponential distributions with respective means μ O and μ C c, is considered. Estimation of (μ O , μ C ) when brief sojourns in the open and closed states fail to be detected is studied. The moment estimating equations for (μ O , μ C ) have no, one or two solutions, with two being the most common situation. The asymptotic behaviour of method-of-moments estimators of (μ O , μ C ), as the number of sojourns becomes large, is determined, and the resulting confidence regions for (μ O , μ C ) are compared with those derived by Clarke, Milne and Yeo under an approximate model. A new method of discriminating between the two solutions of the moment estimating equations is presented. The theory is illustrated by numerical examples taken from ion channel literature},
